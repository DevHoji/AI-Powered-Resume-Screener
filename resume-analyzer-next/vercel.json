{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "GEMINI_API_KEY": "@gemini_api_key",
    "NEXT_PUBLIC_POSTHOG_KEY": "@posthog_key",
    "NEXT_PUBLIC_POSTHOG_HOST": "@posthog_host"
  }
}
